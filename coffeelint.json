{
	"no_tabs": {
		"level": "ignore"
	},
	"indentation": {
		"level": "ignore"
	},
	"arrow_spacing": {
		"level": "warn"
	},
	"braces_spacing": {
		"spaces": 1,
		"empty_object_spaces": 0,
		"level": "warn"
	},
	"max_line_length": {
		"level": "ignore"
	},
	"colon_assignment_spacing": {
		"spacing": { "left": 0, "right": 1 },
		"level": "warn"
	},
	"cyclomatic_complexity": {
		"level": "warn"
	},
	"eol_last": {
		"level": "warn"
	},
	"no_interpolation_in_single_quotes": {
		"level": "warn"
	},
	"no_unnecessary_double_quotes": {
		"level": "warn"
	},
	"prefer_english_operator": {
		"level": "warn"
	},
	"space_operators": {
		"level": "warn"
	},
	"spacing_after_comma": {
		"level": "warn"
	}
}